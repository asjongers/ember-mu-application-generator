{{#link-to "<%= entitiesName %>.show" model tagName='tr' }}
  <td class="properties__data">{{gr-display-field model=model field='id' }}</td>
  <%= attributes.map( function(property) {
    return "" +
		'  <td class="properties__data">{{gr-display-field model=model field=\'' + property.itemVar +'\' }}</td>';
}).join("\n") %>
    <%=
    relationships.map( function(relationship) {
      return "" +
			  '  {{display-model-relationship model=model.' + relationship.itemVar + ' kind=\''+ relationship.kind + '\' itemRoute=\'' + relationship.itemRoute  + '\' relationRoute=\'' + relationship.itemListRoute + '\' relationFilterKey=\'' + entityName + '\' relationFilterValue=model.id configuredLabel=\'id\'}}';
    } ).join("\n")
    %>
{{/link-to}}
