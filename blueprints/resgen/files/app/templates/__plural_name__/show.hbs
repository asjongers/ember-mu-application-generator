<div class="details">
  <h2 class="title">{{t '<%= entityName %> details'}}</h2>
  <div class="actions">
    {{#link-to "<%= entitiesName %>" class="btn back" title="Return to list" tagName='button'}}{{/link-to}}
    {{#link-to "<%= entitiesName %>.edit" model class="btn edit" title="Edit this item" tagName='button'}}{{/link-to}}
    <button type="button" class="btn delete" title="Delete this item" {{action 'delete' model}}></button>
  </div>
  <table>
    <thead>
      <tr>
        <th>Attribute</th>
        <th>Data</th>
      </tr>
    </thead>
    {{display-<%= entityName %>-attributes model=model}}
  </table>
</div>
